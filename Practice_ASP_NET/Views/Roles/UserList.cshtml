@model IEnumerable<Practice_ASP_NET.Models.User>

<h2>User list</h2>
<table class="table">
@foreach (var user in Model)
{
    <tr>
        <td>
            <a asp-controller="Products" asp-action="UserProducts" asp-route-userID="@user.Id">@user.Email</a>
        </td>
        <td>
            <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Edit</a>
        </td>
    </tr>
}
</table>
